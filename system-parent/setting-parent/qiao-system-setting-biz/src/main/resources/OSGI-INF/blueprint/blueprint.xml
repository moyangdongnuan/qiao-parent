<?xml version="1.0" encoding="UTF-8"?>
<blueprint xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xmlns="http://www.osgi.org/xmlns/blueprint/v1.0.0"
           xsi:schemaLocation="http://www.osgi.org/xmlns/blueprint/v1.0.0 http://www.osgi.org/xmlns/blueprint/v1.0.0/blueprint.xsd">

    <reference id="cacheManager" interface="com.kalix.framework.core.api.cache.ICacheManager"/>
    <reference id="shiroService" interface="com.kalix.framework.core.api.security.IShiroService"/>
    <reference id="qiaoSettingBeanDao" interface="com.kalix.qiao.system.setting.api.dao.IQiaoSettingBeanDao"/>


    <bean id="qiaoSettingBeanServiceImpl" class="com.kalix.qiao.system.setting.biz.QiaoSettingBeanServiceImpl">
        <property name="dao" ref="qiaoSettingBeanDao"/>
        <property name="shiroService" ref="shiroService"/>
        <property name="cacheManager" ref="cacheManager"/>
    </bean>

    <service id="qiaoSettingBeanService" interface="com.kalix.qiao.system.setting.api.biz.IQiaoSettingBeanService"
             ref="qiaoSettingBeanServiceImpl">
    </service>
</blueprint>